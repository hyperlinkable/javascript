var height=parseInt(prompt("What is your height in m?")); //asks for height m
var weight=parseInt(prompt("What is your weight in kg?")); //asks for weight kg
var bmi = function() {return weight / (height * height);}; //returns BMI
bmi();

var bmiDirect = function(height,weight) {return height * weight;}; //to directly state height and weight

var height=parseInt(prompt("What is your height in ft?")); //asks for height ft
var weight=parseInt(prompt("What is your weight in lbs?")); //asks for weight lb
var bmi = function() {return (0.453592 * weight) / ((0.3048 * height) * (0.3048 * height));}; //returns BMI
bmi();
